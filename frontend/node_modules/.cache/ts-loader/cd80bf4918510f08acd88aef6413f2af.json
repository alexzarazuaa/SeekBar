    path: \"/bares\",\n    name: \"SBbarsList\",\n    component: SBbarsList,\n  },\n  {\n    path: \"/bar/:slug\",\n    name: \"SBbar\",\n    props:true,\n    component: SBbar,\n  },\n  {\n    path:\"/create\",\n    name:\"SBcreateBar\",\n    component:SBcreateBar,\n\n  \n  },\n  {\n    path: \"/:checkType/:username\",\n    name: \"SBprofile\",\n    props: true,\n    beforeEnter: authGuard,\n    component: SBprofile,\n\n  },\n  {\n    path: \"/:catchAll(.*)\",\n    redirect: \"/\",\n  },\n];\n\nconst router = createRouter({\n  history: createWebHistory(process.env.BASE_URL),\n  routes,\n});\n\nexport default router;\n"],"sourceRoot":""}]}